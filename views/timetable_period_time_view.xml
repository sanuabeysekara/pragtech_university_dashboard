<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <record id="timetable_period_time_view_tree" model="ir.ui.view">
            <field name="name">timetable.period.time.tree</field>
            <field name="model">timetable.period.time</field>
            <field name="arch" type="xml">
                <tree string="Timetable Time Master" editable="bottom">
                    <field name="name"/>
                    <field name="start_time"/>
                    <field name="end_time"/>
<!--                    <field name="sequence"/>-->
                </tree>
            </field>
        </record>

        <record id="action_view_timetable_period_time" model="ir.actions.act_window">
            <field name="name">Period Timings</field>
            <field name="res_model">timetable.period.time</field>
            <field name="view_mode">tree</field>
        </record>

    </data>
</odoo>
