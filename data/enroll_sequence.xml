<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record id="enroll_sequence" model="ir.sequence">
            <field name="name">Enroll No</field>
            <field name="code">enroll.mechanism</field>
            <field name="prefix">E</field>
            <field name="padding">4</field>
        </record>
        <record id="student_id_seq" model="ir.sequence">
            <field name="name">Student Id</field>
            <field name="code">student.id.generate</field>
            <field name="prefix">STU</field>
            <field name="padding">4</field>
            <field eval="1" name="number_increment"/>
            <field eval="False" name="company_id"/>
        </record>
        <record id="teacher_id_seq" model="ir.sequence">
            <field name="name">Teacher Id</field>
            <field name="code">teacher.id.generate</field>
            <field name="prefix">T</field>
            <field name="padding">4</field>
            <field eval="1" name="number_increment"/>
            <field eval="False" name="company_id"/>
        </record>

    </data>
</odoo>